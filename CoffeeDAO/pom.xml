	<repositories>
			<repository>
				<id>mvn2</id>
				<url>http://repo1.maven.org/maven2/</url>
				<releases>
					<enabled>true</enabled>
				</releases>
				<snapshots>
					<enabled>true</enabled>
				</snapshots>
			</repository>
			
			<repository>
				<id>egovframe</id>
				<url>http://www.egovframe.go.kr/maven/</url>
				<releases>
					<enabled>true</enabled>
				</releases>
				<snapshots>
					<enabled>false</enabled>
				</snapshots>
			</repository>
		</repositories>

